{
    "name": "prologuetech/big",
    "type": "library",
    "description": "Google BigQuery for Laravel",
    "keywords": [
        "Prologue Technology",
        "Big",
        "Google",
        "BigQuery"
    ],
    "homepage": "https://github.com/prolougetech/laravel-big",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Percifield",
            "email": "epercifield@prologuetechnology.com",
            "homepage": "http://prologuetechnology.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/cache": "^5.4",
        "php" : "~7.0",
        "google/cloud": "^0.51"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Prolougetech\\Big\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prolougetech\\Big\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "config": {
        "sort-packages": true,
        "minimum-stability": "dev",
        "prefer-stable": true
    }
}
